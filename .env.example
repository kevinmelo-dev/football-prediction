# Token da API da football-data.org
API_TOKEN=your_api_token

# Ligas desejadas (separadas por vírgula) — ex: BSA,PL,SA
LEAGUES=BSA

# Temporadas desejadas (ex: 2023,2024,2025)
SEASONS=2023,2024,2025

# Configurações do banco de dados PostgreSQL (não altere se estiver usando Docker padrão)
DB_HOST=db
DB_PORT=5432
DB_NAME=football
DB_USER=postgres
DB_PASSWORD=postgres
